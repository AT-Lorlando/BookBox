<nav>
    <ul class="pagination">
        @each(anchor in anchors.getUrlsForRange(1, anchors.lastPage))
            <li class="page-item {{ anchors.currentPage == anchor ? 'active' : '' }}">
                <a class="page-link" href="{{ `?page=${anchor.page}` }}">{{ anchor.page }}</a>
            </li>
        @endeach
    </ul>
</nav>